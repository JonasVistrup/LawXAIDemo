BrudtLoven(P,§42 stk.1,T) <- Fører(P,K,T), Hastighed(K,H,T), På(K,V,T), MaxHastighed(V,M,T), <(M,H):§42 stk.1;

MaxHastighed(V,50km/t,T) <- I(V,By),By(By), ~AndenHastighedsGrænse(V):§42 stk. 1.1:§42 stk. 4;
MaxHastighed(V,80km/t,T) <- Landevej(V), ~AndenHastighedsGrænse(V):§42 stk. 1.2;
MaxHastighed(V,130km/t,T) <- Motorvej(V), ~AndenHastighedsGrænse(V):§42 stk. 2;
MaxHastighed(V,50km/t,T) <- Motortrafikvej(V,T), ~AndenHastighedsGrænse(V):§42 stk. 3;
MaxHastighed(V,M,T) <- AndenHastighedsGrænseFastsat(V,M,T):§42 stk. 4:§42 stk. 5;

AndenHastighedsGrænse(V) <- AndenHastighedsGrænseFastsat(V,M,T):§42 stk. 5;
